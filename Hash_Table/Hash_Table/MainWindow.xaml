<Window x:Class="Hash_Table.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hash_Table"
        mc:Ignorable="d"
        Title="G-MAN HashTable" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="keyBox" HorizontalAlignment="Left" Height="46" Margin="52,98,0,0" TextWrapping="Wrap" Text="key" VerticalAlignment="Top" Width="302"/>
        <TextBox x:Name="valueBox" HorizontalAlignment="Left" Height="44" Margin="52,167,0,0" TextWrapping="Wrap" Text="value" VerticalAlignment="Top" Width="302"/>
        <Button x:Name="addButt" Content="Add" HorizontalAlignment="Left" Height="72" Margin="52,256,0,0" VerticalAlignment="Top" Width="187" Click="ADD"/>
        <Button x:Name="showButt" Content="ShowAll" HorizontalAlignment="Left" Height="72" Margin="539,256,0,0" VerticalAlignment="Top" Width="187" Click="SHOWALL"/>
        <Button x:Name="removeButt" Content="Remove" HorizontalAlignment="Left" Height="72" Margin="285,256,0,0" VerticalAlignment="Top" Width="187" Click="REMOVE"/>

    </Grid>
</Window>
